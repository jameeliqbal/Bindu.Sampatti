


@page
@using  Bindu.Sampatti.Localization
@using Bindu.Sampatti.Web.Pages.Procurement.PurchaseRequisition
@using Microsoft.Extensions.Localization
@using Volo.Abp.AspNetCore.Mvc.UI.Bootstrap.TagHelpers.Modal

@model CreateModalModel
@inject IStringLocalizer<SampattiResource> L

@{
    Layout = null;
}

<abp-dynamic-form abp-model="PR" asp-page="/Procurement/PurchaseRequisitions/CreateModal">
    <abp-modal>
        <abp-modal-header title="@L["NewBook"].Value"></abp-modal-header>

        <abp-modal-body>
            <abp-form-content />
        </abp-modal-body>
        <abp-modal-footer buttons="@(AbpModalButtons.Cancel|AbpModalButtons.Save)"></abp-modal-footer>
    </abp-modal>
</abp-dynamic-form>
