@page

@using  Bindu.Sampatti.Web.Pages.Assets.Register

@model IndexModel

@section scripts
{
    <abp-script src="/Pages/AssetManagement/Asset/Index.js"/>
}

<abp-card>
    <abp-card-header>
        <abp-row>
            <abp-column size-md="_6">
                <abp-card-title>
                   Asset List
                </abp-card-title>
            </abp-column>
            <abp-column size-md="_6" class="text-right">
                <abp-button id="NewFixedAsset"  text="Add New Asset" icon="plus" button-type="Primary"/>
            </abp-column>
        </abp-row>
    </abp-card-header>
    <abp-card-body >
        <abp-table striped-rows="true" hoverable-rows="true" id="assetList" />
    </abp-card-body>
</abp-card>


 