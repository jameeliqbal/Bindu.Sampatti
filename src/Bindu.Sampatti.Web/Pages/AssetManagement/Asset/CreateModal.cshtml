@page

@using Bindu.Sampatti.Localization
@using Microsoft.Extensions.Localization
@using Volo.Abp.AspNetCore.Mvc.UI.Bootstrap.TagHelpers.Modal  
@using Bindu.Sampatti.Web.Pages.AssetManagement.Asset

@model CreateModalModel
@inject IStringLocalizer<SampattiResource> L

@{
    Layout = null;
}

<abp-dynamic-form abp-model="Asset" asp-page="/assetmanagement/Asset/CreateModal">
    <abp-modal>
        <abp-modal-header title="@L["NewAsset"].Value"></abp-modal-header>

        <abp-modal-body>
            <abp-form-content />
        </abp-modal-body>

        <abp-modal-footer buttons="@(AbpModalButtons.Save|AbpModalButtons.Cancel)">

        </abp-modal-footer>

    </abp-modal>
</abp-dynamic-form>