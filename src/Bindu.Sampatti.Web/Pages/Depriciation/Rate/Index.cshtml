@page

@using Bindu.Sampatti.Web.Pages.Depriciation.Rate

@model IndexModel


@section scripts
{
    <abp-script src="/pages/Depriciation/Rate/index.js" />
}

@{
    ViewData["Title"] = "Depriciation Rates - Depriciation - Sampatti";
}

<abp-card>
    <abp-card-header>
        <abp-row>
            <abp-column size-md="_6">
                <abp-card-title>
                    Depriciation Rates
                </abp-card-title>
            </abp-column>
            <abp-column size-md="_6" class="text-right">
                <abp-button id="NewDepriciationRate" text="New Depriciation Rate" icon="plus" button-type="Primary" />

            </abp-column>
        </abp-row>
    </abp-card-header>

    <abp-card-body>
        <abp-table striped-rows="true" hoverable-rows="true" id="rateList" />
    </abp-card-body>
</abp-card>